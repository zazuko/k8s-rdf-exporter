{
  "name": "@zazuko/k8s-rdf-exporter",
  "version": "0.4.0",
  "description": "Kubernetes RDF exporter",
  "main": "dist/index.js",
  "bin": "./dist/cli.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/zazuko/k8s-rdf-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/zazuko/k8s-rdf-exporter/issues"
  },
  "scripts": {
    "build": "tsc -p . && chmod +x ./dist/cli.js",
    "start": "npm run build && node ./dist/cli.js",
    "lint": "eslint .",
    "test": "npm run lint",
    "prepublish": "npm run build",
    "prepack": "npm run build",
    "release": "changeset publish"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "kubernetes",
    "rdf",
    "exporter",
    "cli"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/clownface": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.32.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.4.0",
    "@rdfjs/namespace": "^2.0.1",
    "@rdfjs/types": "^2.0.1",
    "@tpluscode/rdf-string": "^1.3.4",
    "@zazuko/env": "^3.0.1",
    "clownface": "^2.0.3",
    "commander": "^14.0.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
